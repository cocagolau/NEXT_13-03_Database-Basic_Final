<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
    <bean id="dealerDao" class="com.dbbasic.dao.impl.JdbcDealerDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
    <bean id="companyDao" class="com.dbbasic.dao.impl.JdbcCompanyDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
    <bean id="carDao" class="com.dbbasic.dao.impl.JdbcCarDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
    <bean id="usedcarDao" class="com.dbbasic.dao.impl.JdbcUsedcarDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
    <bean id="accidentDao" class="com.dbbasic.dao.impl.JdbcAccidentDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
    <bean id="photoDao" class="com.dbbasic.dao.impl.JdbcPhotoDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
    <bean id="subviewDao" class="com.dbbasic.dao.impl.JdbcSubviewDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

 
</beans>